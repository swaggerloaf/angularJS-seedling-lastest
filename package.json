{
  "name": "angularJS-seedling-latest",
  "version": "1.0.0",
  "author": "@swaggerloaf",
  "description": "Starter angularJS project using webpack and bootstrap",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --watch",
    "build": "node_modules/.bin/webpack --progress",
    "pretest": "yarn install",
    "test": "karma start karma.conf.js",
    "test-single-run": "karma start karma.conf.js --single-run"
  },
  "license": "MIT",
  "repository": "https://github.com/swaggerloaf/angularJS-seedling-latest",
  "dependencies": {
    "angular": "~1.7.7",
    "angular-animate": "~1.7.7",
    "angular-resource": "~1.7.7",
    "angular-sanitize": "~1.7.7",
    "angular-touch": "~1.7.7",
    "@uirouter/angularjs": "1.0.22",
    "angular-ui-bootstrap": "2.5.6",
    "angular-toastr": "2.1.1",
    "blueimp-file-upload": "9.28.0",
    "bootstrap": "3.3.7",
    "font-awesome": "4.7.0"
  },
  "devDependencies": {
    "@types/angular": "^1.6.54",
    "@types/angular-mocks": "1.7.0",
    "@types/angular-resource": "1.5.15",
    "@types/angular-animate": "1.5.10",
    "angular-mocks": "1.7.7",
    "eslint": "^5.14.1",
    "eslint-plugin-import": "2.16.0",
    "prettier": "^1.16.4",
    "sass-loader": "7.1.0",
    "webpack": "4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "karma": "^4.0.0",
    "protractor": "^5.4.2",
    "jasmine-core": "^3.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^2.0.1",
    "jasmine": "^3.3.1"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jasmine": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn"
    },
    "settings": {}
  },
  "keywords": [
    "babel",
    "angular",
    "webpack"
  ]
}
